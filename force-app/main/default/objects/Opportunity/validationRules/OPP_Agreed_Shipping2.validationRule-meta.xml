<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OPP_Agreed_Shipping2</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;Agreed Shipping&quot;),
ISPICKVAL(Tipo__c,&quot;Quote&quot;),

Total_Quotes_Vendido__c  &lt;= 0 

)</errorConditionFormula>
    <errorMessage>OPP005- Se necesita Quote Vendido</errorMessage>
</ValidationRule>
