<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Approved_Without_Cargo_Lines</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL( Quotation_Status__c, &quot;Approved as Succesful&quot;) &amp;&amp; 
Total_Items_Units__c = 0</errorConditionFormula>
    <errorMessage>!!ERROR: Must be Contain &quot;CARGO LINES&quot;</errorMessage>
</ValidationRule>
