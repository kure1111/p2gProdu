    <decisions>
        <description>Detecta si es un servicio NACIONAL o INTERNACIONAL</description>
        <name>NACIONAL_INTERNACIONAL_1</name>
        <label>NACIONAL/INTERNACIONAL</label>
        <locationX>314</locationX>
        <locationY>575</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>NACIONAL_1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.FolioResume__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>FN</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Gerente_Comercial_FN</targetReference>
            </connector>
            <label>NACIONAL</label>
        </rules>
        <rules>
            <name>INTERNACIONAL_1</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>FI</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>PTO</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>M</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.Name</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>A</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Import_Export_Quote__r.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>R</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Gerente_Comercial_INTER</targetReference>
            </connector>
            <label>INTERNACIONAL</label>
        </rules>
    </decisions>
