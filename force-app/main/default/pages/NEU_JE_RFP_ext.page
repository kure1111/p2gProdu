<script type="text/javascript">
        var n$ = jQuery.noConflict();
        n$(document).ready(function (){
            updateInterface(false);
            setvisited();
            n$('#ranking').flightboard({maxLength:15,repeat:false,lettersImage:'{!$Resource.flightboard_png}',lettersSeq:' ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-'});
            updatestatic();
            updateauction();
        });
        function updateInterface()
        {
            if(n$(n$.sfId('{!$Component.form.block1.error}')).getString()!='')
            {
                alert(n$(n$.sfId('{!$Component.form.block1.error}')).getString());
                if(n$(n$.sfId('{!$Component.form.block1.error}')).getString().indexOf('Valid Until')>0)
                    n$('.valid_until').css('border','2px solid red');
                if(n$(n$.sfId('{!$Component.form.block1.error}')).getString().indexOf('Production Time')>0)
                    n$('.production_time').css('border','2px solid red');
                    
               if(n$(n$.sfId('{!$Component.form.block1.error}')).getString().indexOf('Sent')>0)//prueba si actualiza el error
               {
                    n$('.seleccionar_status').css('border','2px solid red');
                    n$('.documentos_obligatorios').css('border','2px solid red');
                    n$('.seleccionar_status').val('Draft');
               }
                n$(n$.sfId('{!$Component.form.block1.error}')).val('');
            }
            n$("#accordion").accordion({heightStyle:"content",active:false,collapsible:true});
            n$("#accordion .ui-accordion-header input:text").on('click',function(){return false;});
            n$("#accordion .ui-accordion-header").off('keydown');
            n$("#accordion .ui-accordion-header input:text").on('focus',function(){
                if(n$(this).getString()!='')
                    return true;
                var element=n$(this).attr('id');
                n$('#accordion .ui-accordion-header input:text').each(function(i){
                    if(n$(this).attr('id')==element)
                    {
                        n$('#accordion').accordion('option','active',i);
                        return false;
                    }
                    });
                return true;
                });
            n$('#accordion .ui-accordion-header input:text').each(function(i){
                if(n$(this).getString()=='')
                {
                    n$('#accordion').accordion('option','active',i);
                    return false;
                }
                });
            updatestatic();
        }
        var oldranking;
        function updatestatic()
        {
            n$(n$.sfId('{!$Component.form.block1.staticauction.statica.staticstatus}')).html(n$(n$.sfId('{!$Component.form.block1.dynamicstateauctiontext}')).val());
            n$('#ranking').flightboard({messages:[n$(n$.sfId('{!$Component.form.block1.ranking}')).val()],maxLength:15,repeat:false,lettersImage:'{!$Resource.flightboard_png}',lettersSeq:' ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-'});
            if(oldranking!=n$(n$.sfId('{!$Component.form.block1.ranking}')).val())
            {
                oldranking=n$(n$.sfId('{!$Component.form.block1.ranking}')).val();
                n$('#ranking').flightboard('option','messages',[oldranking]);
                n$('#ranking').flightboard('flip'); 
            }
        }
        var nextrefreshauction=1000;
        function updateauction()
        {
            var myTime=n$('#mytime').getInt();
            if(myTime==0)
            {
                myTime=(new Date()).getTime();
                n$('#mytime').val(myTime);
            }
            switch(n$(n$.sfId('{!$Component.form.block1.dynamicstateauction}')).getInt())
            {
                case 1:
                    if(n$('#progressbar').hasClass('ui-progressbar'))
                        n$('#progressbar').progressbar('destroy');
                    var auctionTime=myTime+n$(n$.sfId('{!$Component.form.block1.startauction}')).getInt()-(new Date()).getTime();
                    if(auctionTime>0)
                    {
                        n$(n$.sfId('{!$Component.form.block1.auction.as.startauction}')).html(timeleft(auctionTime));
                        setTimeout(function(){updateauction();},1000);
                        nextrefreshauction=1000;
                        return;
                    }
                    setTimeout(function(){refreshauction();},nextrefreshauction);
                    nextrefreshauction=5000;
                    return;
                case 2:
                    nextrefreshauction=1000;
                    var auctionTime=myTime+n$(n$.sfId('{!$Component.form.block1.closeauction}')).getInt()-(new Date()).getTime();
                    if(auctionTime>0)
                    {
                        if(!n$('#progressbar').hasClass('ui-progressbar'))
                            n$('#progressbar').progressbar({value:false});
                        if(n$(n$.sfId('{!$Component.form.block1.startauction}')).getInt()!=0)
                        {
                            n$('#progressbar').progressbar('option','max',n$(n$.sfId('{!$Component.form.block1.closeauction}')).getInt()-n$(n$.sfId('{!$Component.form.block1.startauction}')).getInt());
                            n$('#progressbar').progressbar('option','value',-n$(n$.sfId('{!$Component.form.block1.startauction}')).getInt()-myTime+(new Date()).getTime());
                        }
                        n$(n$.sfId('{!$Component.form.block1.auction.ac.closeauction}')).html(timeleft(auctionTime));
                    }
                    else if(n$('#progressbar').hasClass('ui-progressbar'))
                        n$('#progressbar').progressbar('destroy');
                    setTimeout(function(){updateauction();},1000);
                    return;
                default:
                    if(n$('#progressbar').hasClass('ui-progressbar'))
                        n$('#progressbar').progressbar('destroy');
                    nextrefreshauction=1000;
            }
        }
        function timeleft(milisec)
        {
            var secs=Math.floor(milisec/1000);
            var res='';
            var current=Math.floor(secs/31536000);//Years
            secs-=current*31536000;
            if(current>0)
            {
                res=current+'&nbsp;year';
                if(current!=1) res+='s';
            }
            current=Math.floor(secs/2592000);//Months
            secs-=current*2592000;
            if(current>0)
            {
                if(res!='') res+=', ';
                res=res+current+'&nbsp;month';
                if(current!=1) res+='s';
            }
            current=Math.floor(secs/86400);//Days
            secs-=current*86400;
            if(current>0)
            {
                if(res!='') res+=', ';
                res=res+current+'&nbsp;day';
                if(current!=1) res+='s';
            }
            current=Math.floor(secs/3600);//Hours
            secs-=current*3600;
            if(current>0)
            {
                if(res!='') res+=', ';
                res=res+current+'&nbsp;hour';
                if(current!=1) res+='s';
            }
            current=Math.floor(secs/60);//Minutes
            secs-=current*60;
            if(current>0)
            {
                if(res!='') res+=', ';
                res=res+current+'&nbsp;minute';
                if(current!=1) res+='s';
            }
            secs=Math.floor(secs);//Seconds
            if(secs>0)
            {
                if(res!='') res+=', ';
                res=res+secs+'&nbsp;second';
                if(secs!=1) res+='s';
            }
            return res;
        }
        
