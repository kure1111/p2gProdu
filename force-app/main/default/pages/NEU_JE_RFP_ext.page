        function copydata(value)
        {
        	n$('#suppdata'+value+' .peso_items_supplier').val(n$('#suppdata'+(value-1)+' .peso_items_supplier').val());
        	n$('#suppdata'+value+' .volumen_packaging_length').val(n$('#suppdata'+(value-1)+' .volumen_packaging_length').val());
