        	n$('#suppdata'+value+' .volumen_packaging_width').val(n$('#suppdata'+(value-1)+' .volumen_packaging_width').val());
        	n$('#suppdata'+value+' .peso_packaging_weight').val(n$('#suppdata'+(value-1)+' .peso_packaging_weight').val());
        	n$('#suppdata'+value+' .volumen_packaging_height').val(n$('#suppdata'+(value-1)+' .volumen_packaging_height').val());
        	n$('#suppdata'+value+' .unidades_x_caja').val(n$('#suppdata'+(value-1)+' .unidades_x_caja').val());
        }
</script>
