        { 
          if(n$('.volumen_packaging_length').val() != ''|| n$('.volumen_packaging_width').val() != ''||  n$('.volumen_packaging_height').val() != '')
            {
                n$('.volumen_packaging_length').css('border-left-color','red');
                n$('.volumen_packaging_width').css('border-left-color','red');
                n$('.volumen_packaging_height').css('border-left-color','red');
                n$('.unidades_x_caja').css('border-left-color','red');
            }
            
            if(n$('.volumen_packaging_length').val() == '' && n$('.volumen_packaging_width').val() == '' &&  n$('.volumen_packaging_height').val() == '' )
            {
                n$('.volumen_packaging_length').css('border-left-color','white');
                n$('.volumen_packaging_width').css('border-left-color','white');
                n$('.volumen_packaging_height').css('border-left-color','white');
            }
            
            if(n$('.peso_packaging_weight').val() != '')
            {
                n$('.peso_items_supplier').css('border-left-color','red');
                n$('.unidades_x_caja').css('border-left-color','red');
            }
            else
            {
                n$('.peso_items_supplier').css('border-left-color','white');
            }     
             
             if( n$('.volumen_packaging_length').val() != ''|| n$('.volumen_packaging_width').val() != ''||  n$('.volumen_packaging_height').val() != '' ||   n$('.peso_items_supplier').val() !='')
