<apex:repeat value="{!itemstosuplay}" var="item">
    <apex:pageBlockSection showHeader="false" columns="4">
        <apex:pageBlockSectionItem labelStyle="text-align:left">
            <apex:outputLabel value="{!item.line.Supplier_Request_Line__r.Item_Name__c}"/>
            <apex:outputLabel />
        </apex:pageBlockSectionItem>
        
        <apex:outputField label="Quantity{!IF(item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c != null, ' ('+item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c+')', IF(item.line.Supplier_Request_Line__r.Unit_of_measure__c!=null,' ('+item.line.Supplier_Request_Line__r.Unit_of_measure__c+')',''))}" value="{!item.line.Quantity__c}" rendered="{!item.line.Supplier_Request_Line__r.Partial_Quote__c == false || isopen == false}"/>
        
        <apex:inputField label="Quantity{!IF(item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c != null, ' ('+item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c+')',IF(item.line.Supplier_Request_Line__r.Unit_of_measure__c!=null,' ('+item.line.Supplier_Request_Line__r.Unit_of_measure__c+')',''))}" value="{!item.line.Quantity__c}" style="text-align: right;" html-placeholder="{!item.quantity_request_line}" rendered="{!item.line.Supplier_Request_Line__r.Partial_Quote__c == true && isopen == true && item.line.Supplier_Request_Line__r.Hide_Quantity__c == false}"/><!-- {!item.quantity_request_line} -->
        
        <apex:inputField label="Quantity{!IF(item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c != null, ' ('+item.line.Supplier_Request_Line__r.Unit_of_measure_RFP__c+')',IF(item.line.Supplier_Request_Line__r.Unit_of_measure__c!=null,' ('+item.line.Supplier_Request_Line__r.Unit_of_measure__c+')',''))}" value="{!item.line.Quantity__c}" style="text-align: right;"  rendered="{!item.line.Supplier_Request_Line__r.Partial_Quote__c == true && isopen == true && item.line.Supplier_Request_Line__r.Hide_Quantity__c == true}"/> 
       
        
       <!--  <apex:outputField label="Target Price{!IF(quote.Incoterm__c!=null,' ('+quote.Incoterm__c+')','')}" value="{!item.line.Supplier_Request_Line__r.Target_Price__c}" rendered="{!item.line.Supplier_Request_Line__r.Target_Price__c!=null}"/>-->
        <apex:outputField label="Target Price{!IF(quote.Incoterm__c!=null,' ('+quote.Incoterm__c+')','')}" value="{!item.line.Supplier_Request_Line__r.Target_Price__c}" rendered="{!item.line.Supplier_Request_Line__r.Show_Target_Price__c==true}"/>
        
        <apex:pageBlockSectionItem rendered="{!item.line.Supplier_Request_Line__r.Show_Target_Price__c!=true}">
            <apex:outputLabel />
            <apex:outputLabel />
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem rendered="{!isopen}">
            <apex:outputLabel value="Your Best Unit Price{!IF(quote.Incoterm__c!=null,' ('+quote.Incoterm__c+')','')}"/>
            <apex:pageBlockSectionItem >
               <!--  <apex:outputLabel value="({!item.line.CurrencyIsoCode})"/>-->
               <apex:outputLabel value="({!item.moneda_selected})"/>
                <apex:inputField value="{!item.line.Price__c}" style="text-align: right; padding-right: 5px;"/>
            </apex:pageBlockSectionItem>
        </apex:pageBlockSectionItem>
        <apex:outputField label="Your Best Unit Price{!IF(quote.Incoterm__c!=null,' ('+quote.Incoterm__c+')','')}" value="{!item.line.Price__c}" rendered="{!!isopen}"/>
    </apex:pageBlockSection>
    <div>
        <apex:pageBlockSection title="Item Data" columns="3">
            <apex:pageBlockSectionItem dataStyle="width:0;vertical-align:middle">
                <apex:outputField value="{!item.line.Supplier_Request_Line__r.Photo__c}"/>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem dataStyle="width:50%">
                <apex:pageBlockSection columns="1">
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Family_Name__c}" rendered="{!item.line.Supplier_Request_Line__r.Family_Name__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Subfamily_Name__c}" rendered="{!item.line.Supplier_Request_Line__r.Subfamily_Name__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Item_Name__c}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Item_Code__c}" rendered="{!item.line.Supplier_Request_Line__r.Item_Code__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.HS_Code__c}" rendered="{!item.line.Supplier_Request_Line__r.HS_Code__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Description__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Description__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Weight_kg__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Weight_kg__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Item_Units_Packaging__c}" rendered="{!item.line.Supplier_Request_Line__r.Item_Units_Packaging__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Gross_Weight_kg__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Gross_Weight_kg__c!=null}"/>
                </apex:pageBlockSection>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem dataStyle="width:50%">
                <apex:pageBlockSection columns="1">
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Technical_Data__c}" rendered="{!item.line.Supplier_Request_Line__r.Technical_Data__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Technical_description__c}" rendered="{!item.line.Supplier_Request_Line__r.Technical_description__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Image__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Image__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_System__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_System__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Sizes_Image__c}" rendered="{!item.line.Supplier_Request_Line__r.Sizes_Image__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Length_cm__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Length_cm__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Width_cm__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Width_cm__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Height_cm__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Height_cm__c!=null}"/>
                    <apex:outputField value="{!item.line.Supplier_Request_Line__r.Packaging_Volume_m3__c}" rendered="{!item.line.Supplier_Request_Line__r.Packaging_Volume_m3__c!=null}"/>
                </apex:pageBlockSection>
            </apex:pageBlockSectionItem>
        </apex:pageBlockSection>
        <div id="suppdata{!item.linenumber}" style="position:relative">
       	<apex:commandLink onclick="copydata({!item.linenumber});return false;" style="position:absolute;left:16em;top:.5em" rendered="{!(isopen)&&(item.linenumber!=1)}">
       		<apex:image value="{!$Resource.copydata}"/>
       	</apex:commandLink>
        <apex:pageBlockSection title="Supplier Packaging Data">
            <apex:inputField value="{!item.line.Supplier_Item_Weight_kg__c}" rendered="{!isopen}" html-class="peso_items_supplier" onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Supplier_Item_Weight_kg__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Packaging_Length_cm__c}" rendered="{!isopen}" html-class="volumen_packaging_length" onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Packaging_Length_cm__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Packaging_Description__c}" rendered="{!isopen}" html-class="Packaging_Description"/>
            <apex:outputField value="{!item.line.Packaging_Description__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Packaging_Width_cm__c}" rendered="{!isopen}" html-class="volumen_packaging_width"  onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Packaging_Width_cm__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Packaging_Weight_kg__c}" rendered="{!isopen}" html-class="peso_packaging_weight" onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Packaging_Weight_kg__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Packaging_Height_cm__c}" rendered="{!isopen}" html-class="volumen_packaging_height" onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Packaging_Height_cm__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Item_Units_x_Packaging__c}" rendered="{!isopen}" html-class="unidades_x_caja" onKeyUp="comprobar_campos_requeridos();"/>
            <apex:outputField value="{!item.line.Item_Units_x_Packaging__c}" rendered="{!!isopen}"/>
            <apex:outputField value="{!item.line.Packaging_Volume_m3__c}" rendered="{!(item.line.Packaging_Volume_m3__c!=null)&&(item.line.Packaging_Volume_m3__c!=0)}"/>
            <apex:outputLabel rendered="{!(item.line.Packaging_Volume_m3__c=null)||(item.line.Packaging_Volume_m3__c=0)}"/>
            <apex:outputField value="{!item.line.Packaging_Gross_Weight_kg__c}" rendered="{!(item.line.Packaging_Gross_Weight_kg__c!=null)&&(item.line.Packaging_Gross_Weight_kg__c!=0)}"/>
            <apex:outputLabel rendered="{!(item.line.Packaging_Gross_Weight_kg__c=null)||(item.line.Packaging_Gross_Weight_kg__c=0)}"/>
            <apex:outputField value="{!item.line.Supplier_Total_Volume_m3__c}" rendered="{!(item.line.Supplier_Total_Volume_m3__c!=null)&&(item.line.Supplier_Total_Volume_m3__c!=0)}"/>
            <apex:outputLabel rendered="{!(item.line.Supplier_Total_Volume_m3__c=null)||(item.line.Supplier_Total_Volume_m3__c=0)}"/>
            <apex:outputField value="{!item.line.Packaging_Quantity__c}" rendered="{!(item.line.Packaging_Quantity__c!=null)&&(item.line.Packaging_Quantity__c!=0)}"/>
            <apex:outputLabel rendered="{!(item.line.Packaging_Quantity__c=null)||(item.line.Packaging_Quantity__c=0)}"/>
            <apex:outputLabel />
            <apex:outputField value="{!item.line.Supplier_Total_Weight_kg__c}" rendered="{!(item.line.Supplier_Total_Weight_kg__c!=null)&&(item.line.Supplier_Total_Weight_kg__c!=0)}"/>
            <apex:outputLabel rendered="{!(item.line.Supplier_Total_Weight_kg__c=null)||(item.line.Supplier_Total_Weight_kg__c=0)}"/>
        </apex:pageBlockSection>
        </div>
        <apex:pageBlockSection title="Supplier Item Observations">
            <apex:inputField value="{!item.line.Production_Time_Days__c}" rendered="{!isopen}" style="border-left: 2px solid red;" styleClass="production_time"/>
            <apex:outputField value="{!item.line.Production_Time_Days__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Expense_Concept__c}" rendered="{!isopen}"/>
            <apex:outputField value="{!item.line.Expense_Concept__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Observations__c}" rendered="{!isopen}"/>
            <apex:outputField value="{!item.line.Observations__c}" rendered="{!!isopen}"/>
            <apex:inputField value="{!item.line.Expense_Amount__c}" rendered="{!isopen}"/>
            <apex:outputField value="{!item.line.Expense_Amount__c}" rendered="{!!isopen}"/>
        </apex:pageBlockSection>
        <apex:pageBlockSection title="Evaluation Criterias" rendered="{!item.evaluationcriterias!=null}">
        <apex:repeat value="{!item.evaluationcriterias}" var="crit">
            <apex:pageBlockSectionItem >
                <apex:outputLabel value="{!crit.Name}"/>
                <apex:pageBlockSectionItem >
                    <apex:selectRadio value="{!crit.selected}" readonly="{!!isopen}" disabled="{!!isopen}">
                        <apex:selectOptions value="{!crit.options}"/>
                    </apex:selectRadio>
                    <apex:outputLabel value="{!crit.Description}" rendered="{!crit.Description!=null}"/>
                </apex:pageBlockSectionItem>
            </apex:pageBlockSectionItem>
        </apex:repeat>
        </apex:pageBlockSection>
    </div>
</apex:repeat>
</div>
</apex:pageBlock>
<apex:actionstatus id="myStatus">
    <apex:facet name="start">
        <div class="waitingSearchDiv" id="el_loading" style="background-color:#fbfbfb;height:100%;opacity:0.65;width:100%;"> 
            <div class="waitingHolder" style="top:74.2px;width:91px;">
                <img class="waitingImage" src="/img/loading.gif" title="Please Wait..." />
                <span class="waitingDescription">Loading...</span>
            </div>
        </div>
    </apex:facet>
</apex:actionstatus>
<div class="waitingSearchDiv" id="otherStatus" style="display:none;background-color:#fbfbfb;height:100%;opacity:0.65;width:100%;"> 
    <div class="waitingHolder" style="top:74.2px;width:91px;">
        <img class="waitingImage" src="/img/loading.gif" title="Please Wait..." />
        <span class="waitingDescription">Loading...</span>
    </div>
</div>
</apex:form>
</apex:define>
</apex:composition>
</apex:page>