                return;
            }
            lanzar_ventana();
        }
        
        function lanzar_ventana()
        {
          updateInterface(false);
          if(n$(n$.sfId('{!$Component.form.block1.error}')).getString() =='')
             if(window.open("{!$Site.BaseUrl}/apex/NEU_Associated_10Documents_Request?id="+n$(n$.sfId('{!$Component.form.block1.quoteid}')).getString()+"&docu="+doc_number, "_blank", "top=500, left=500, width=465, height=280") == undefined)
                alert('We could not open the document upload window.\nPlease, check the popup blocker or try again');
            
            
        }
        
        function update_quote()
