        function creineas()
        {   
            var todos_rellenos = true;
            if($('.numlinea').val() == '')
            {
                $('.numlinea').css('border','1px solid red');
                todos_rellenos = false;
            }
            else
                $('.numlinea').css('border','1px solid gray');
            if($('.extensionname').val() == '')
            {
