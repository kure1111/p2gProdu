    <script>  
        $(document).ready(function () {
            $("#accordion").accordion({heightStyle:"content",active:false, activate:function(event,ui){}});//  
            setDecimalSep($($.sfId('{!$Component.form.decimalSep}')).val(),$($.sfId('{!$Component.form.thousandSep}')).val());
