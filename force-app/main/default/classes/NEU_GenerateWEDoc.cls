public with sharing class NEU_GenerateWEDoc 
{
    public Shipment__c record;
    public string doc {get;set;}
    
    ApexPages.StandardController con;
    
    public NEU_GenerateWEDoc(ApexPages.StandardController stdController) 
    {
        con = stdController;
        
        try 
        {
            record = [select Id, Name, Warehouse_Movement_Type__c, Total_Units_Shipped__c, Supplier_Account__c, Supplier_Account__r.Name, Account_for__c, Account_for__r.Name,
                      Contact__c, Contact__r.Name, Vehicle__c, Vehicle__r.Num_Placa_Tracto__c, Vehicle__r.Num_Placa_Caja__c, National_Operator__c, National_Operator__r.Name,
                      Solicitud_de_Maniobras__c, National_Operator__r.Numlicvig_ope__c
                      from Shipment__c where Id =: con.getRecord().Id];
        } 
        catch(Exception ee){record = new Shipment__c();}
        
        List<Inventory_Entry__c> entries = [select Id, Name, Item__c, Item__r.Name, Units_Entry__c, Shipment_Item_Line__c, Shipment_Item_Line__r.Pallet_Code__c,
                                            Item__r.Sourcing_Item_Code__c, Shipment_Item_Line__r.pallet_warehouse__r.Id, Shipment_Item_Line__r.pallet_warehouse__r.Name
                                            from Inventory_Entry__c where Shipment__c =: record.Id  order by Shipment_Item_Line__r.pallet_warehouse__r.Id asc];


        List<Inventory_Exit__c> exits = [select Id, Name, Item__c, Item__r.Name, Units_Picked__c, Shipment_Item_Line__c, Shipment_Item_Line__r.Pallet_Code__c,
                                         Item__r.Sourcing_Item_Code__c, Shipment_Item_Line__r.pallet_warehouse__r.Id, Shipment_Item_Line__r.pallet_warehouse__r.Name
                                         from Inventory_Exit__c where Shipment__c =: record.Id order by Shipment_Item_Line__r.pallet_warehouse__r.Id];

        System.debug(exits);
        
        doc = '';
        string header = '';
        string pie = '';
        string QRfolio = '';
        
        header += '<div style="page-break-after: always;">';
        header += '<table class="main_table">';
        header += '	<tr>';
        header += '		<th colspan="3" align="center">'+(record.Warehouse_Movement_Type__c == 'Entries (In)' ? 'RECEPCIÓN' : (record.Warehouse_Movement_Type__c == 'Deliveries (Out)' ? 'EMBARQUE' : ''))+'</th>';
        header += '	</tr>';
        header += '	<tr>';
        header += '		<th rowspan="2" align="center" class="logo_th"><img src="/resource/logo_pak2go_notifications"/></th>';
        header += '		<td colspan="2"><strong>N° FOLIO</strong> '+record.Name+'</td>';
        header += '	</tr>';
        header += '	<tr>';
        header += '		<td colspan="2"><strong>SOLICITUD DE MANIOBRAS '+(record.Solicitud_de_Maniobras__c ? '(SI)' : '(NO)')+'</strong></td>';
        header += '	</tr>';
        header += '	<tr>';
        header += '		<td><strong>FECHA:</strong> '+system.today().day()+'-'+system.today().month()+'-'+system.today().year()+'</td>';
        header += '		<td colspan="2"><strong>N°- DE TARIMAS / BULTOS</strong> '+(record.Total_Units_Shipped__c != null ? record.Total_Units_Shipped__c.format() : '')+'</td>';
        header += '	</tr>';
        header += '	<tr>';
        header += '		<td colspan="2"><strong>PROVEEDOR:</strong> '+(record.Supplier_Account__c != null ? record.Supplier_Account__r.Name : '')+'</td>';
        header += '		<td><strong>N°- FOLIO DE DOC:</strong> '+record.Name+'</td>';
        header += '	</tr>';
        header += '	<tr>';
        header += '		<td colspan="2"><strong>CLIENTE:</strong> '+(record.Account_for__c != null ? record.Account_for__r.Name : '')+'</td>';
        header += '		<td><strong>CONTACTO:</strong> '+(record.Contact__c != null ? record.Contact__r.Name : '')+'</td>';
        header += '	</tr>';
        header += '</table>';
        header += '<table class="main_table">';
        header += '	<tr>';
        header += '		<th align="center">PALLET</th>';
        header += '		<th align="center">DESCRIPCION DE MERCANCIA</th>';
        header += '		<th align="center">CODIGO</th>';
        header += '		<th align="center">CANTIDAD RECIBIDA</th>';
        header += '		<th align="center">'+(record.Warehouse_Movement_Type__c == 'Entries (In)' ? 'INVENTARIO' : (record.Warehouse_Movement_Type__c == 'Deliveries (Out)' ? 'PENDIENTE DE SERVIR' : ''))+'</th>';
        header += '	</tr>';
        
        pie += '<table class="main_table">';
        pie += '	<tr>';
        pie += '		<td rowspan="2"><strong>OBSERVACIONES:</strong></td>';
        pie += '		<td><strong>PLACAS DE TRACTOR</strong></td>';
        pie += '		<td>'+(record.Vehicle__c != null && record.Vehicle__r.Num_Placa_Tracto__c != null ? record.Vehicle__r.Num_Placa_Tracto__c : '')+'</td>';
        pie += '	</tr>';
        pie += '	<tr>';
        pie += '		<td><strong>PLACAS DE CAJA</strong></td>';
        pie += '		<td>'+(record.Vehicle__c != null && record.Vehicle__r.Num_Placa_Caja__c != null ? record.Vehicle__r.Num_Placa_Caja__c : '')+'</td>';
        pie += '	</tr>';
        pie += '	<tr>';
        pie += '		<th rowspan="2"><strong>DATOS DEL OPERADOR</strong></th>';
        pie += '		<td colspan="2">'+(record.National_Operator__c != null ? record.National_Operator__r.Name : '')+'</td>';
        pie += '	</tr>';
        pie += '	<tr>';
        pie += '		<td colspan="2">'+(record.National_Operator__c != null && record.National_Operator__r.Numlicvig_ope__c != null ? record.National_Operator__r.Numlicvig_ope__c : '')+'</td>';
        pie += '	</tr>';
        pie += '</table>';
        pie += '</div>';

        integer max_lines = 20;
        integer cont = 0;
        Id palletCodeOld ;
        List<String> palletCodesQR = new List<String>();
        List<Id> palletIdQR = new List<Id>();


        doc += header;
        
        //mostramos solo entradas o salidas segun el movement type		
        if (record.Warehouse_Movement_Type__c == 'Entries (In)')
        {
            for(Inventory_Entry__c e : entries)
            {
                if(cont == max_lines)
                {
                    if(entries.size() > 0)
                        doc += '</table>';
                    
                    doc += pie;
                    doc += header;
                    
                    cont = 0;
                }
                
                doc += '	<tr>';
                
                if(palletCodeOld != e.Shipment_Item_Line__r.pallet_warehouse__r.Id ||  palletCodeOld == null)
                {
                    doc += '		<td>'+(e.Shipment_Item_Line__c != null && e.Shipment_Item_Line__r.pallet_warehouse__r.Name != null ? e.Shipment_Item_Line__r.pallet_warehouse__r.Name: '')+'</td>';
                    palletCodesQR.add(e.Shipment_Item_Line__r.pallet_warehouse__r.Name);
                    if(e.Shipment_Item_Line__r.pallet_warehouse__r.Id != null)
                        palletIdQR.add(e.Shipment_Item_Line__r.pallet_warehouse__r.Id);
                }
                else
                {
                    doc += '		<td></td>';
                }
                
                doc += '		<td>'+(e.Item__c != null ? e.Item__r.Name : '')+'</td>';
                doc += '		<td>'+(e.Item__c != null && e.Item__r.Sourcing_Item_Code__c != null ? e.Item__r.Sourcing_Item_Code__c : '')+'</td>';
                doc += '		<td>'+(e.Units_Entry__c != null ? e.Units_Entry__c.format() : '0')+' </td>';
                doc += '		<td>'+(e.Units_Entry__c != null ? e.Units_Entry__c.format() : '0')+' </td>';
                doc += '	</tr>';
                
                palletCodeOld = e.Shipment_Item_Line__r.pallet_warehouse__r.Id;
                cont++;
            }
        }
        
        if (record.Warehouse_Movement_Type__c == 'Deliveries (Out)')
        {
            for(Inventory_Exit__c e : exits)
            {
                if(cont == max_lines)
                {
                    if(exits.size() > 0)
                        doc += '</table>';
                    
                    doc += pie;
                    doc += header;
                    
                    cont = 0;
                }
                
                doc += '	<tr>';
                if(palletCodeOld != e.Shipment_Item_Line__r.pallet_warehouse__r.Id ||  palletCodeOld == null)
                {
                    doc += '		<td>'+(e.Shipment_Item_Line__c != null && e.Shipment_Item_Line__r.pallet_warehouse__r.Name != null ? e.Shipment_Item_Line__r.pallet_warehouse__r.Name: '')+'</td>';
                    palletCodesQR.add(e.Shipment_Item_Line__r.pallet_warehouse__r.Name);
                    if(e.Shipment_Item_Line__r.pallet_warehouse__r.Id != null)
                        palletIdQR.add(e.Shipment_Item_Line__r.pallet_warehouse__r.Id);
                }
                else
                {
                    doc += '		<td></td>';
                }
                doc += '		<td>'+(e.Item__c != null ? e.Item__r.Name : '')+'</td>';
                doc += '		<td>'+(e.Item__c != null && e.Item__r.Sourcing_Item_Code__c != null ? e.Item__r.Sourcing_Item_Code__c : '')+'</td>';
                doc += '		<td>'+(e.Units_Picked__c != null ? e.Units_Picked__c.format() : '0')+' </td>';
                doc += '		<td>'+(e.Units_Picked__c != null ? e.Units_Picked__c.format() : '0')+' </td>';
                doc += '	</tr>';

                palletCodeOld = e.Shipment_Item_Line__r.pallet_warehouse__r.Id;
                cont++;
            }
        }
        while(cont < max_lines)
        {
            doc += '	<tr>';
            doc += '		<td>&nbsp;</td>';
            doc += '		<td>&nbsp;</td>';
            doc += '		<td>&nbsp;</td>';
            doc += '		<td>&nbsp;</td>';
            doc += '		<td>&nbsp;</td>';
            doc += '	</tr>';
            
            cont++;
        }
        
        if(entries.size() > 0)
            doc += '</table>';
        
        doc += pie;
        
        if(palletCodesQR.size() > 0)
        {
            String urlQR = '';
            integer contadorQR = 0;
            Decimal contadorQR2 = 0;
            integer numDiv = 0;

            System.debug(palletIdQR);
            System.debug(palletCodesQR);


            for(String e : palletCodesQR)
            {
                if(e != null && e != '')
                {
                    urlQR = 'http://chart.apis.google.com/chart?cht=qr&chs=300x300&chl=https://pak2go--c.eu17.visual.force.com/NEU_QR_Response?shipment='+con.getRecord().Id + '%26pallet='+palletIdQR[contadorQR];
                contadorQR2 = contadorQR/4;
                /*if(contadorQR == 0 || palletCodesQR.size() <= 4)
                {
                    QRfolio += '		<div><span class="titleQR">** Recorte y pegue los códigos QR en el pallet correspondiente</span><br/><br/><br/><br/><br/><br/>';
                    numDiv = 0;
                }   
                else if(contadorQR == 0 || contadorQR2.scale() <= 1)
                {
                    QRfolio += '		<div style="page-break-after: always;"><span class="titleQR">** Recorte y pegue los códigos QR en el pallet correspondiente</span><br/><br/><br/><br/><br/><br/>';
                    numDiv = 0;
                }*/
                QRfolio += '<div style="page-break-before: always;"><span class="titleQR">** Recorte y pegue los códigos QR en el pallet correspondiente</span><br/><br/><br/><br/><br/><br/>';
                QRfolio += '	<div id="divisor'+numDiv+'" class="divisorQR" style="border-style: dotted;border-width: medium;">';
                QRfolio += '	<h2 class="qrTitle">'+e+ '</h2>';
                QRfolio += '	<img src="'+urlQR+'" class="QRImage">';
                QRfolio += '	</div>';
                QRfolio += '	<div id="divisor'+numDiv+'" class="divisorQR" style="border-style: dotted;border-width: medium;">';
                QRfolio += '	<h2 class="qrTitle">'+e+'</h2>';
                QRfolio += '	<img src="'+urlQR+'" class="QRImage">';
                QRfolio += '	</div>';
                QRfolio += '	<div id="divisor'+numDiv+'" class="divisorQR" style="border-style: dotted;border-width: medium;">';
                QRfolio += '	<h2 class="qrTitle">'+e+'</h2>';
                QRfolio += '	<img src="'+urlQR+'" class="QRImage">';
                QRfolio += '	</div>';
                QRfolio += '	<div id="divisor'+numDiv+'" class="divisorQR" style="border-style: dotted;border-width: medium;">';
                QRfolio += '	<h2 class="qrTitle">'+e+'</h2>';
                QRfolio += '	<img src="'+urlQR+'" class="QRImage">';
                QRfolio += '	</div>';
                
                //if(contadorQR == 0 || contadorQR2.scale() <= 1)
                //{
                    QRfolio += '</div>';                
                //}  
                numDiv++;
                contadorQR++;
                }
            }
            
            
            doc+=QRfolio;
        }
    }



