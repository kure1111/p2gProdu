global class PAK_WS_ACUSE {
    
    global class ACUSE{
        webservice String Shipment {get;set;}
        webservice String Fecha {get;set;}
        webservice String Status {get;set;}
        webservice String VoBo {get;set;}
    }
    
    global class status {
        webservice String code {get; set;}
        webservice String message {get; set;}
        webservice ACUSE registry {get; set;}
    }
	
    webservice static list<status> USHIP(ACUSE[] lsEntity){
    	list<status> result = new list<status>();
        try{
            for(ACUSE A: lsEntity){
                for(Shipment__c S: [Select Id,Acuse_en_SAP__c,Date_Acuse_SAP__c,Status_Acuse_SAP__c,VoBo_Acuse_SAP__c From Shipment__c Where Name=:A.Shipment]){
                    if(A.Fecha != null && A.Fecha != ''){
                        S.Acuse_en_SAP__c = true;
                    	S.Date_Acuse_SAP__c = Date.valueOf(A.Fecha);
                    }else{
                        S.Acuse_en_SAP__c = false;
                    }
                    S.Status_Acuse_SAP__c = A.Status;
                    if(A.VoBo == '1'){S.VoBo_Acuse_SAP__c = true;}else if(A.VoBo == '0'){S.VoBo_Acuse_SAP__c = false;}
                    
                    update S;
                }
                	status st = new status();
                    st.code = '1';
                    st.message = 'Exitoso- el registro fue actualizado';
                    st.registry = A;
                    result.add(st);
            }
        	return result;            
        }catch(Exception e){
            System.debug(e.getCause()); System.debug(e.getMessage()); System.debug(e.getLineNumber());
			status st = new status(); st.code = '0'; st.message = 'Existe un error en el web services, por favor revise la trasabilidad de la transacciÃ³n' + e.getCause() +' '+ e.getMessage() +' '+ e.getLineNumber();				
			result.add(st);
            
			return result;
        }

    }
